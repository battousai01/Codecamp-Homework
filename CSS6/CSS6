      <script>
        function updateCircles() {
          // Get the selected value
          /*const selectedValue = parseInt(
            document.getElementById("level").value
          );*/
          const levelCard1 = document.getElementById("level-card1").value;
          const levelCard2 = document.getElementById("level-card2").value;

          // Reset all circles to light gray
          document.querySelectorAll(".circle").forEach((circle) => {
            circle.classList.remove("active");
          });

          // Activate circles up to the selected value
          /*for (let i = 1; i <= selectedValue; i++) {
            document.getElementById(`circle${i}`).classList.add("active");
          }*/
          // Activate circles up to the selected value in level-card1
          const levelCard1Section = document
            .getElementById("level-card1")
            .closest(".card-content");
          const circlesInLevelCard1 =
            levelCard1Section.querySelectorAll(".circle");
          for (let i = 0; i < parseInt(levelCard1); i++) {
            circlesInLevelCard1[i].classList.add("active");
          }
          // Activate circles up to the selected value in level-card2
          const levelCard2Section = document
            .getElementById("level-card2")
            .closest(".card-content");
          const circlesInLevelCard2 =
            levelCard2Section.querySelectorAll(".circle");
          for (let i = 0; i < parseInt(levelCard2); i++) {
            circlesInLevelCard2[i].classList.add("active");
          }
        }
      </script>
    </main>
